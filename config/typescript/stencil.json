{
  "extends": "./base.json",
  "compilerOptions": {
    "declaration": false,
    "experimentalDecorators": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true
  }
}
